{
  "name": "mythras-sw-charsheet",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "npm run build && wrangler deploy",
    "dev": "react-router dev",
    "preview": "vite preview",
    "start": "wrangler dev",
    "typecheck": "wrangler types && react-router typegen && tsc -b",
    "combot:dev": "eval \"$(aws ssm get-parameter --name combot-secrets --with-decryption --query 'Parameter.Value' --output text | sed 's/^/export /')\" && bun run --watch combot/combot.ts",
    "combot:register": "eval \"$(aws ssm get-parameter --name combot-secrets --with-decryption --query 'Parameter.Value' --output text | sed 's/^/export /')\" && bun run combot/register-commands.ts"
  },
  "dependencies": {
    "alchemy": "^0.76.1",
    "discord-interactions": "^4.4.0",
    "dotenv": "^17.2.3",
    "effect": "^3.18.4",
    "hono": "^4.10.2",
    "isbot": "^5.1.17",
    "itty-aws": "^0.4.1",
    "lucide-react": "^0.483.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^0.1.11",
    "@cloudflare/workers-types": "^4.20250303.0",
    "@react-router/dev": "^7.3.0",
    "@react-router/fs-routes": "^7.9.4",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "4.42.2"
  }
}